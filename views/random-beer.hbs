<h1>{{beer.name}}</h1>

<div id="randomBeerWrapper">
        {{>beerpartial beer}}
        <h3>Food pairing:</h3>
        <ul>
            {{#each beer.food_pairing}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
        <p><span>Brewer Tips: </span>{{beer.brewer_tips}}</p>
</div>